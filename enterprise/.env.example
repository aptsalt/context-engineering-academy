# Enterprise API Configuration
# Copy this file to .env.local and fill in your values

# API key for enterprise endpoints (required)
ENTERPRISE_API_KEY=

# LLM provider for contextualization engine
OPENAI_API_KEY=

# Vector database for storing ingested content embeddings
VECTOR_DB_URL=http://localhost:6333
VECTOR_DB_COLLECTION=enterprise-content

# Enterprise org defaults
DEFAULT_ORG_ID=

# Enable enterprise API (set to any value to enable)
NEXT_PUBLIC_ENTERPRISE_API_URL=
